{
  "name": "phaser",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --content-base phaser/dist/",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "genMigration": "ts-node $(npm bin)/typeorm migration:generate -n Initialize",
    "upMigration": "ts-node $(npm bin)/typeorm migration:run",
    "downMigration": "ts-node $(npm bin)/typeorm migration:revert"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "pg": "^8.0.0",
    "phaser": "^3.21.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
